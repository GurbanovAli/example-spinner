{"frames": { 
{% for sprite in allSprites %}
"{{sprite.trimmedName}}": 
{
    {% if sprite.rotated %}"frame": { "x":{{sprite.frameRect.x}},"y":{{sprite.frameRect.y}},"w":{{sprite.frameRect.height}},"h":{{sprite.frameRect.width}}}, {% else %}"frame": { "x":{{sprite.frameRect.x}},"y":{{sprite.frameRect.y}},"w":{{sprite.frameRect.width}},"h":{{sprite.frameRect.height}}}, {% endif %}
	"rotated": {{sprite.rotated}},
	"trimmed": {{sprite.trimmed}},
	"spriteSourceSize": {"x":{{sprite.cornerOffset.x}},"y":{{sprite.cornerOffset.y}},"w":{{sprite.size.width}},"h":{{sprite.size.height}}},
	"sourceSize": {"w":{{sprite.untrimmedSize.width}},"h":{{sprite.untrimmedSize.height}}}
}{% if not forloop.last %}, {% endif %}{% endfor %}},
"meta": {
    "image": "{{texture.fullName}}",
	"size": {"w":{{texture.size.width}},"h":{{texture.size.height}}},
    "scale": "{{variantParams.scale}}"
}
}
